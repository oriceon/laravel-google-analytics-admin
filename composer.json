{
    "name": "tda/laravel-google-analytics-admin",
    "description": "Google Analytics Admin integration with oAuh2",
    "keywords": [
        "tda",
        "laravel",
        "analyitcs admin",
        "google analyitcs"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "DL",
            "email": "davileichs@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tda\\LaravelGoogleAnalyticsAdmins\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tda\\LaravelGoogleAnalyticsAdmin\\GoogleAnalyticsAdminServiceProvider"
            ],
            "aliases": {
                "GoogleAnalyticsAdmin": "Tda\\LaravelGoogleAnalyticsAdmin\\Facades\\GoogleAnalyticsAdmin"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
